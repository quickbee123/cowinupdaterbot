SET timezone = 'Asia/Kolkata';

CREATE TABLE cowin (
    id serial PRIMARY KEY,
    user_id INT NOT NULL UNIQUE,
    last_sent TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
    age INT DEFAULT 0,
    dose INT DEFAULT 0,
    vaccine INT DEFAULT 0,
    fee INT DEFAULT 0
);

CREATE TABLE district(
    user_id INT NOT NULL,
    district_id INT NOT NULL,
    PRIMARY KEY (user_id,district_id)
);

CREATE TABLE pincode(
    user_id INT NOT NULL,
    pincode_num INT NOT NULL,
    PRIMARY KEY (user_id,pincode_num)
);




users

*id INT
*user_id INT
*last_sent DATETIME
*age INT
*dose INT
*vaccine INT
*fee INT

district

*district_id INT
*user_id INT


pincode

*pincode_num INT
*user_id INT


Age
0-Both
1-18+
2-45+

Dose
0-Both
1-one
2-two

Vaccine
0-Any
1-Covishield
2-Covaxin
3-Sputnik

Fee
0-Both
1-Free
2-Paid